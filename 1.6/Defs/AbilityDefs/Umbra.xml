<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
<VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_ShadowStrikeSTNL</defName>
	<label>Shadow Claw</label>
	<description>Strike the target with a claw made of psychic energy, targets in the dark will receive an additional hit.\n\n(100% Psychic Sensitivity = 10 Damage || Targets must be in a cell with less than/equal to 30% light to take a second hit.)\n\n(Darklight is considered as darkness for this ability.)</description>
	<iconPath>UI_UmbraSTNL/Abilities/ShadowClaw</iconPath>
	<castTime>30</castTime>
	<targetMode>Pawn</targetMode>
	<!-- <chance>0</chance> -->
	<range>18.9</range>
	<needsTicking>true</needsTicking>
	<goodwillImpact>0</goodwillImpact>
	<abilityClass>VPE_Umbra.Ability_ShadowStrike</abilityClass>
	<castSound>PsycastPsychicPulse</castSound>
	<!-- <fleckOnTarget>PsychicRitual_Complete</fleckOnTarget> -->
	<targetingParameters>
		<canTargetLocations>False</canTargetLocations>
		<canTargetSelf>True</canTargetSelf>
		<canTargetAnimals>True</canTargetAnimals>
		<canTargetPawns>True</canTargetPawns>
		<canTargetHumans>True</canTargetHumans>
		<canTargetMechs>True</canTargetMechs>
	</targetingParameters>
	<modExtensions>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
			<path>VPE_UmbraSTNL</path>
			<level>1</level>
			<order>1</order>
			<psyfocusCost>0.03</psyfocusCost>
			<entropyGain>10</entropyGain>
		</li>
		<!-- <li Class="VEF.Abilities.AbilityExtension_EffecterOnTarget"> -->
		<!-- <effecterDef>RaisedRock_Collapse</effecterDef> -->
		<!-- <maintainForTicks>1</maintainForTicks> -->
		<!-- <scale>1</scale> -->
		<!-- </li> -->
	</modExtensions>
</VEF.Abilities.AbilityDef>

<!-- Level 2 -->

<VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_UmbralCurseSTNL</defName>
	<label>Curse</label>
	<description>Sap the energy from the target by cursing them with a looming psychic phenomenon that feeds on their life force.\n\nSubsequent casts will further decrease the maximum capacity for the target's stats up to a maximum of 30%.\n\nCasting this ability on a target that is standing in a location with a light level of 30% or less will instantly afflict them with the full effect of Curse.\n\n(Darklight is considered as darkness for this ability.)</description>
	<iconPath>UI_UmbraSTNL/Abilities/Curse</iconPath>
	<castTime>45</castTime>
	<targetMode>Pawn</targetMode>
	<durationTime>5000</durationTime>
	<durationTimeStatFactors>
		<PsychicSensitivity>1</PsychicSensitivity>
	</durationTimeStatFactors>
	<!-- <chance>0</chance> -->
	<range>21.9</range>
	<needsTicking>true</needsTicking>
	<goodwillImpact>-10</goodwillImpact>
	<abilityClass>VPE_Umbra.Ability_Curse</abilityClass>
	<castSound>VPE_WordOfFear_Cast</castSound>
	<targetingParameters>
		<canTargetLocations>False</canTargetLocations>
		<canTargetSelf>True</canTargetSelf>
		<canTargetAnimals>True</canTargetAnimals>
		<canTargetPawns>True</canTargetPawns>
		<canTargetHumans>True</canTargetHumans>
		<canTargetMechs>True</canTargetMechs>
	</targetingParameters>
	<modExtensions>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
			<path>VPE_UmbraSTNL</path>
			<level>2</level>
			<order>2</order>
			<psyfocusCost>0.05</psyfocusCost>
			<entropyGain>15</entropyGain>
			<prerequisites>
				<li>VPE_ShadowStrikeSTNL</li>
			</prerequisites>
		</li>
	</modExtensions>
</VEF.Abilities.AbilityDef>
  
  <!-- Level 3 -->  
  
  <VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
	<defName>VPE_ConcealSkipSTNL</defName>
	<label>Conceal Skip</label>
	<description>Turn yourself into a shadow and emerge at the target location, nearby allies will become shrouded in darkness as the caster reforms, rendering them invisible.</description>
	<iconPath>UI_UmbraSTNL/Abilities/ConcealingSkip</iconPath>
	<castTime>30</castTime>
	<targetMode>Pawn</targetMode>
	<chance>0</chance>
	<range>25.9</range>
	<radius>3.5</radius>
	<needsTicking>true</needsTicking>
	<goodwillImpact>0</goodwillImpact>
	<abilityClass>VPE_Umbra.Ability_ConcealSkip</abilityClass>
	<castSound>PsycastPsychicPulse</castSound>
	<!-- <fleckOnTarget>PsychicRitual_Complete</fleckOnTarget> -->
	<targetingParameters>
		<canTargetLocations>True</canTargetLocations>
		<canTargetSelf>False</canTargetSelf>
