			<path>VPE_UmbraSTNL</path>
			<level>6</level>
			<order>1</order>
			<psyfocusCost>0.70</psyfocusCost>
			<entropyGain>50</entropyGain>
			<prerequisites>
			<li>VPE_ShadowFangSTNL</li>
			</prerequisites>
		</li>
		<li Class="VanillaPsycastsExpanded.AbilityExtension_GameCondition">
			<gameCondition>VPE_UmbraPsychicDarknessSTNL</gameCondition>
			<durationDays>1</durationDays>
